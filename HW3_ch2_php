<?php

	header("Content-Type:text/html; charset=big5");
	$data1= $_GET["data1"];
	echo "<center>";
	echo"<caption><font size='5'>$data1 質數列表</font></caption>";
	echo "<table width='50' height='20' border='2'>";
	for($s=2;$s<$data1;$s++){
		$t=1;

        echo "<tr>";

        for($y=1;$y<=$s;$y++){
			 if ($s % $y == 0)
				{
					$t++;                  
				}  
        }
		if ($t<=3)
			{
				
				echo"<td bgcolor = yellow> $s </td> ";
 
			}    

		echo "</tr>";
    }
	echo "</table>";
	echo "</center>";
?>
